<%= form_for @chef do |f| %>
  <%= f.label :name %> <br>
  <%= f.text_field :name %> <hr>
  <%= f.label :phone_number %> <br>
  <%= f.text_field :phone_number %><hr/>
  <%= f.label :password %> <br>
  <%= f.password_field :password %> <hr>
  <%= f.label :confirm %> <br>
  <%= f.password_field :password_confirmation %><br/>
  <hr>
  <%= f.submit %>

<% end %>
